[alias]
	s = status
	a = "add"
	c = "commit"
	lol = "log --graph --decorate --pretty=oneline --all --abbrev-commit"
	pall = "pull --all"
	fall = "fetch --all"
	pu = "pull upstream"
	po = "pull origin"
	lgss = "log --graph --date=short --decorate=short --all"
        # Untracked filesを表示しない
	so  = "status -uno"
        # 単語レベルでの差分
	wdiff = "diff --word-diff"
